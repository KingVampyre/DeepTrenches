{
  "type": "features:generator",
  "rotations": "all",
  "pools": [
    {
      "type": "features:pool",
      "pool": "deep_trenches:trees/peltogyne/trunks",
      "setters": [
        {
          "parameter": "deep_trenches:height",
          "getters": [
            {
              "getter": "deep_trenches:multiplier",
              "multiplier": 10,
              "value": {
                "parameter": "deep_trenches:index"
              }
            }
          ]
        }
      ],
      "functions": [
        {
          "function": "features:pivot",
          "when": {
            "all": {
              "down": 3
            }
          }
        }
      ]
    }
  ]
}