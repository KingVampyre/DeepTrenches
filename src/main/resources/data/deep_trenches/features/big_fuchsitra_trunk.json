{
  "type": "features:jigsaw",
  "pools": [
    {
      "type": "features:pool",
      "pool": "deep_trenches:trees/fuchsitra/trunks",
      "context": {
        "type": "features:tree",
        "overrides": [
          {
            "type": "features:parameter",
            "parameter": "features:height",
            "from": [
              {
                "type": "features:multiply",
                "multiplier": 4,
                "value": {
                  "parameter": "features:trunks"
                }
              }
            ]
          }
        ]
      },
      "functions": [
        {
          "function": "features:pivot",
          "when": {
            "all": {
              "down": 2
            }
          }
        }
      ]
    }
  ]
}